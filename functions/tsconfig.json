{
  "compilerOptions": {
    // "module": "commonjs",
    "module": "ES2022",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "dist",
    "sourceMap": true,
    "strict": true,
    "target": "es2022",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "checkJs": false
  },
  "compileOnSave": true,
  // "include": [
  //   "src"
  // ]
  "include": ["./src/**/*.ts", "./src/**/*.tsx"],
  "exclude": ["src/**/*.js", "src/**/*.jsx"]
}
