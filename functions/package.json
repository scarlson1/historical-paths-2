{
  "name": "functions",
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "rm -rf ./dist/ && tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "emulators:dev": "firebase emulators:start --only functions,auth",
    "setSAPath": "export GOOGLE_APPLICATION_CREDENTIALS=\"/Users/spencercarlson/Documents/dev/service_accounts/historical-paths-admin.json\"",
    "dev": "concurrently \"npm run setSAPath\" \"npm run emulators:dev\"  \"npm run build:watch\"",
    "dev:old": "npm run setSAPath && npm run emulators:dev && npm run build:watch"
  },
  "engines": {
    "node": "20"
  },
  "main": "dist/index.js",
  "dependencies": {
    "@turf/turf": "^6.5.0",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.3.1",
    "install": "^0.13.0",
    "lodash-es": "^4.17.21",
    "npm": "^10.5.0",
    "pg": "^8.11.3",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/pg": "^8.11.4",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^3.1.0",
    "typescript": "^4.9.0"
  },
  "private": true
}
